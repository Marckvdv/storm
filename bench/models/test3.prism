// Messy Transitions Model
mdp

const double x;

module test3
	s: [0..4] init 0;

	[a] s=0 -> x:(s'=1) + (1-x):(s'=2);
	[b] s=0 -> x:(s'=3) + (1-x):(s'=4);

	[a] s=1 -> x:(s'=0) + (1-x):(s'=3);
	[b] s=1 -> x:(s'=0) + (1-x):(s'=2);

	[a] s=2 -> x:(s'=0) + (1-x):(s'=4);
	[b] s=2 -> x:(s'=0) + (1-x):(s'=1);

	[a] s=3 -> x:(s'=0) + (1-x):(s'=1);
	[b] s=3 -> x:(s'=0) + (1-x):(s'=4);

	[a] s=4 -> x:(s'=0) + (1-x):(s'=2);
	[b] s=4 -> x:(s'=0) + (1-x):(s'=3);
endmodule
